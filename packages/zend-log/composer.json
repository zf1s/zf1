        "zf1s/zend-wildfire": "Used in special situations or with special adapters"
    },
    "replace": {
        "zf1/zend-log": "^1.12"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
