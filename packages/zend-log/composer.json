    "keywords": ["framework", "zf1", "zend", "log"],
    "homepage": "http://framework.zend.com/",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.3.3",
        "ext-reflection": "*",
        "zf1s/zend-exception": "^1.14.0"
    },
    "autoload": {
        "psr-0": {
            "Zend_Log": "library/"
        }
