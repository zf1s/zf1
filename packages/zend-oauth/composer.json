{
    "name": "zf1s/zend-oauth",
    "description": "Zend Framework 1 Oauth package",
    "keywords": ["framework", "zf1", "zend", "oauth"],
    "homepage": "http://framework.zend.com/",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.3.3",
        "zf1s/zend-exception": "^1.15.5",
        "zf1s/zend-http": "^1.15.5",
        "zf1s/zend-uri": "^1.15.5",
        "zf1s/zend-crypt": "^1.15.5"
    },
    "autoload": {
        "psr-0": {
            "Zend_Oauth": "library/"
        }
    },
    "suggest": {
        "zf1s/zend-config": "Used in special situations or with special adapters"
    },
    "replace": {
        "zf1/zend-oauth": "^1.12"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
