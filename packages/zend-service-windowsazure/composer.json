{
    "name": "zf1s/zend-service-windowsazure",
    "description": "Zend Framework 1 Service WindowsAzure package",
    "keywords": ["framework", "zf1", "zend", "service", "windowsazure"],
    "homepage": "http://framework.zend.com/",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.3.3",
        "ext-mbstring": "*",
        "zf1s/zend-service": "^1.15.5",
        "zf1s/zend-xml": "^1.15.5"
    },
    "autoload": {
        "psr-0": {
            "Zend_Service_WindowsAzure": "library/",
            "Zend_Service_SqlAzure": "library/"
        }
    },
    "suggest": {
        "zf1s/zend-log": "Used in special situations or with special adapters"
    },
    "replace": {
        "zf1/zend-service-windowsazure": "^1.12"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
