{
    "name": "zf1s/zend-json",
    "description": "Zend Framework 1 Json package",
    "keywords": ["framework", "zf1", "zend", "json"],
    "homepage": "http://framework.zend.com/",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.3.3",
        "ext-reflection": "*",
        "zf1s/zend-exception": "^1.13.4",
