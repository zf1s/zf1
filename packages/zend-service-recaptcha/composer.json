{
    "name": "zf1s/zend-service-recaptcha",
    "description": "Zend Framework 1 Service ReCaptcha package",
    "keywords": ["framework", "zf1", "zend", "service", "recaptcha"],
    "homepage": "http://framework.zend.com/",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.3.3",
        "zf1s/zend-exception": "^1.13.4",
        "zf1s/zend-http": "^1.13.4",
        "zf1s/zend-json": "^1.13.4"
    },
    "autoload": {
        "psr-0": {
            "Zend_Service_ReCaptcha": "library/"
        }
    },
    "suggest": {
        "ext-mcrypt": "Used in special situations or with special adapters (Zend_Service_ReCaptcha_MailHide)"
    },
    "replace": {
        "zf1/zend-service-recaptcha": "^1.12"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
