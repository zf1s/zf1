{
    "name": "zf1s/zend-openid",
    "description": "Zend Framework 1 OpenId package",
    "keywords": ["framework", "zf1", "zend", "openid"],
    "homepage": "http://framework.zend.com/",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.3.3",
        "zf1s/zend-controller": "^1.15.1",
        "zf1s/zend-exception": "^1.15.1",
        "zf1s/zend-http": "^1.15.1",
        "zf1s/zend-session": "^1.15.1"
    },
    "autoload": {
        "psr-0": {
            "Zend_OpenId": "library/"
        }
    },
    "suggest": {
        "ext-bcmath": "Used in special situations or with special adapters (dh key generation)",
        "ext-gmp": "Used in special situations or with special adapters (dh key generation)",
        "ext-openssl": "Used in special situations or with special adapters (dh key generation)"
    },
    "replace": {
        "zf1/zend-openid": "^1.12"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
