{
    "name": "zf1s/zend-test",
    "description": "Zend Framework 1 Test package",
    "keywords": ["framework", "zf1", "zend", "test"],
    "homepage": "http://framework.zend.com/",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.3.3",
        "zf1s/zend-controller": "^1.13.1",
        "zf1s/zend-dom": "^1.13.1",
        "zf1s/zend-exception": "^1.13.1",
        "zf1s/zend-layout": "^1.13.1",
        "zf1s/zend-registry": "^1.13.1",
        "zf1s/zend-session": "^1.13.1"
    },
    "autoload": {
        "psr-0": {
            "Zend_Test": "library/"
        }
    },
    "suggest": {
        "zf1s/zend-loader": "Used in special situations or with special adapters"
    },
    "replace": {
        "zf1/zend-test": "^1.12"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
