{
    "name": "zf1s/zend-mail",
    "description": "Zend Framework 1 Mail package",
    "keywords": ["framework", "zf1", "zend", "mail"],
    "homepage": "http://framework.zend.com/",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.3.3",
        "zf1s/zend-exception": "^1.13",
        "zf1s/zend-loader": "^1.13",
        "zf1s/zend-mime": "^1.13",
        "zf1s/zend-registry": "^1.13",
        "zf1s/zend-validate": "^1.13"
    },
    "autoload": {
        "psr-0": {
            "Zend_Mail": "library/"
        }
    },
    "suggest": {
        "ext-posix": "Used in special situations or with special adapters"
    },
    "replace": {
        "zf1/zend-mail": "^1.12"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
